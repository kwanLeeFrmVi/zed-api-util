{
  "name": "zed-api-util",
  "version": "1.0.0",
  "type": "module",
  "description": "Interactive CRUD tool to manage Zed openai_compatible providers",
  "author": "Quan Le",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kwanLeeFrmVi/zed-api-util.git"
  },
  "homepage": "https://github.com/kwanLeeFrmVi/zed-api-util#readme",
  "bugs": {
